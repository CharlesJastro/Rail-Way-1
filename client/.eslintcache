e navbar\r\n    render() {\r\n        if (this.state.connection === -1) {\r\n            console.log('Error');\r\n            return (\r\n                <div className=\"ui container routesList\">\r\n                    <h2>Routes List Currently Unavailable</h2>\r\n                    <p>Please try again later. We apologize for the inconvenience.</p>\r\n                </div>\r\n            );\r\n        } else if (this.state.connection === 1) {\r\n            return (\r\n                <div className=\"ui container routesList\">\r\n                    <h2>Routes List</h2>\r\n                    <h4>Schedule Timezone: America/Edmonton</h4>\r\n                    <h4>Your Timezone: {DateTime.local().zoneName}</h4>\r\n                    {console.log(DateTime.local().weekday)}\r\n                    <table style={{width:\"100%\"}}>\r\n                        <thead>\r\n                            <tr>\r\n                                <th>Route</th>\r\n                                <th>Departure Time</th>\r\n                                <th>Arrival Time</th>\r\n                                <th>Fare</th>\r\n                            </tr>\r\n                        </thead>\r\n                        <tbody>\r\n                            {this.state.routes.map((route, index) => (\r\n                             <ListItem key={route._id} id={index} data={[route.name, DateTime.fromISO(route.departure).toLocaleString(DateTime.TIME_SIMPLE), DateTime.fromISO(route.arrival).toLocaleString(DateTime.TIME_SIMPLE), route.fare]}/>\r\n                            ))}\r\n                        </tbody>\r\n                    </table>\r\n                </div>\r\n            );\r\n        } else if (this.state.connection === 2) {\r\n            return (\r\n                <div className=\"ui container routesList\">\r\n                    <h2>Routes List</h2>\r\n                    <p>There are no more trains operating today</p>\r\n                </div>\r\n            );\r\n        } else {\r\n            return (\r\n                <div className=\"ui container routesList\">\r\n                    <h2>Routes List</h2>\r\n                    <p>Loading...</p>\r\n                </div>\r\n            );\r\n        }  \r\n    }\r\n};\r\n\r\nexport default RoutesList;\r\n\r\n/* <ListItem key={route._id} id={index} data={[route.name, DateTime.fromISO(route.departure).toLocaleString(DateTime.TIME_SIMPLE), DateTime.fromISO(route.arrival).toLocaleString(DateTime.TIME_SIMPLE), route.fare]}/> */",["96","97"],"C:\\Users\\Charles\\OneDrive\\Desktop\\EvolveU\\Projects\\Project-3\\Rail-Way\\client\\src\\components\\RouteDetails\\RouteDetails.jsx",["98"],"C:\\Users\\Charles\\OneDrive\\Desktop\\EvolveU\\Projects\\Project-3\\Rail-Way\\client\\src\\components\\Header\\Header.jsx",["99"],"C:\\Users\\Charles\\OneDrive\\Desktop\\EvolveU\\Projects\\Project-3\\Rail-Way\\client\\src\\components\\NavBar\\NavBar.jsx",[],"C:\\Users\\Charles\\OneDrive\\Desktop\\EvolveU\\Projects\\Project-3\\Rail-Way\\client\\src\\views\\Favourites\\Favourites.jsx",[],"C:\\Users\\Charles\\OneDrive\\Desktop\\EvolveU\\Projects\\Project-3\\Rail-Way\\client\\src\\views\\Travel\\Travel.jsx",[],"C:\\Users\\Charles\\OneDrive\\Desktop\\EvolveU\\Projects\\Project-3\\Rail-Way\\client\\src\\components\\ListItem\\index.js",[],"C:\\Users\\Charles\\OneDrive\\Desktop\\EvolveU\\Projects\\Project-3\\Rail-Way\\client\\src\\components\\ListSubItem\\index.js",[],"C:\\Users\\Charles\\OneDrive\\Desktop\\EvolveU\\Projects\\Project-3\\Rail-Way\\client\\src\\components\\ListSubItem\\ListSubItem.jsx",[],"C:\\Users\\Charles\\OneDrive\\Desktop\\EvolveU\\Projects\\Project-3\\Rail-Way\\client\\src\\components\\ListItem\\ListItem.jsx",["100"],"import React from 'react';\r\nimport {ListSubItem} from '../ListSubItem';\r\nimport {DateTime} from 'luxon';\r\n\r\n// Route event handler\r\nconst ListItem = ({id, data}) => {\r\n    return (\r\n        <tr id={id} className=\"routeRow\" onClick={()=>window.locati